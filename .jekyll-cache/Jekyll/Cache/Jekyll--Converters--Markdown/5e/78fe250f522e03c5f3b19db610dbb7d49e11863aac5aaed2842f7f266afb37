        t1.Start();
            
        // ì»´íŒŒì¼ëŸ¬ê°€ Run() ë©”ì„œë“œì˜ í•¨ìˆ˜ í”„ë¡œí† íƒ€ì…ìœ¼ë¡œë¶€í„°
        // ThreadStart Delegateê°ì²´ë¥¼ ì¶”ë¡ í•˜ì—¬ ìƒì„±í•¨
        Thread t2 = new Thread(Run);
