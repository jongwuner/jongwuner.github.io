        // ìµëª…ë©”ì„œë“œ(Anonymous Method)ë¥¼ ì‚¬ìš©í•˜ì—¬
        // ì“°ë ˆë“œ ìƒì„±
        Thread t3 = new Thread(delegate()
        {
            Run();
        });
        t3.Start();

        // ëŒë‹¤ì‹ (Lambda Expression)ì„ ì‚¬ìš©í•˜ì—¬
        // ì“°ë ˆë“œ ìƒì„±
        Thread t4 = new Thread(() =&gt; Run());
        t4.Start();

        // ê°„ëµí•œ í‘œí˜„
        new Thread(() =&gt; Run()).Start();
    }

